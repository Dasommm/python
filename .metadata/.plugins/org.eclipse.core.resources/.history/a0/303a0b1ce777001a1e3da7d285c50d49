#산술 연산

a = 21
b =2
print(a+b)
print(a-b)
print(a*b)
print(a**b)
# a의 b제곱
print(a/b)
print(a//b)
#몫 (Floor division)
print(a%b)
#나머지

#비교연산
a,b = 5,3
print(a == b)
print(a != b)
print(a > b)
print(a <= b)
print("========")
print(a is b)
print(a is not b)
print("========")
print(True or False)
print(False and True)
print(not False)
print("========")

#범위 연산
list01 = list(range(100))   #0~99까지 
print(list01)
print(list01[2])














